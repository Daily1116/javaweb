        	<result name="locallanmudetail" type="freemarker">/WEB-INF/wap/index/locallanmudetail.ftl</result>
        	<result name="locallanmudetailall" type="freemarker">/WEB-INF/wap/index/locallanmudetailall.ftl</result>
        	<result name="localhotlist" type="freemarker">/WEB-INF/wap/index/localhotlist.ftl</result>
			<!--整合搜素-->
