        	<result name="detail" type="freemarker">/WEB-INF/wap/live/detail_info.ftl</result>
        	<result name="imageList" type="freemarker">/WEB-INF/wap/live/all_pics.ftl</result>
        	<result name="commentList" type="freemarker">/WEB-INF/wap/live/comment_list.ftl</result>
        	<result name="branchList" type="freemarker">/WEB-INF/wap/live/branch_list.ftl</result>
        	<result name="aroundList" type="freemarker">/WEB-INF/wap/live/around_list.ftl</result>
			<result name="commentSend" type="freemarker">/WEB-INF/wap/live/submit_comment.ftl</result>
			<result name="jiucuo" type="freemarker">/WEB-INF/wap/live/error.ftl</result>
			<result name="changecity" type="freemarker">/WEB-INF/wap/live/change_city.ftl</result>
			<result name="upLoadPic" type="freemarker">/WEB-INF/wap/live/upload_pics.ftl</result>
			<result name="merchantMap" type="freemarker">/WEB-INF/wap/live/map.ftl</result>
			<result name="merchantCoupon" type="freemarker">/WEB-INF/wap/live/coupon_download.ftl</result>
			<result name="topListDetail" type="freemarker">/WEB-INF/wap/live/toplist_detail.ftl</result>
			
		</action>
		
		<!-- WWW生活 -->
		<action name="andlive" class="andLiveAction">
			<result name="index" type="freemarker">/WEB-INF/www/live/index.ftl</result>
			<result name="index_list" type="freemarker">/WEB-INF/www/live/index_list.ftl</result>
			<result name="topListDetail" type="freemarker">/WEB-INF/www/live/rank_list.ftl</result>
			<result name="commentList" type="freemarker">/WEB-INF/www/live/comment.ftl</result>
