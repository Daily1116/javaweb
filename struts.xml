      <!-- json -->
	<package namespace="/json" name="ajax" extends="json-default">
		<!-- 热线 -->
		<action name="hot" class="hotlineAction">
        	<result name="sendError" type="json">
				<param name="includeProperties">result</param>
			</result>
        	
		</action>
		
		
		<!-- 出行 -->
		<action name="tra" class="trafficAction">
        	<result name="tips" type="json">
				<param name="includeProperties">tipList.*</param>
			</result>
        	
        	<result name="sms" type="json">
				<param name="includeProperties">msg</param>
			</result>
        	
        	<result name="del" type="json">
				<param name="includeProperties">total</param>
			</result>
			
			<result name="localCity" type="json">
				<param name="includeProperties">cityName</param>
			</result>
		</action>
		<!-- 会员 -->
		<action name="user" class="userInfoAction">
        	<result name="registerValidate" type="json">
				<param name="includeProperties">error</param>
			</result>
			<result name="gotoTrvale" type="json">
				<param name="includeProperties">rurl</param>
			</result>
			
			<result name="centerCommon" type="json">
				<param name="includeProperties">error,rurl</param>
			</result>
		</action>
		<!-- 公共会员 -->
		<action name="userc" class="userCommAction">
        	<result name="comm_registervalidate" type="json">
				<param name="includeProperties">error</param>
			</result>
			<result name="success" type="json">
				<param name="includeProperties">error,rurl</param>
			</result>
			<result name="comm_lock_num" type="json">
				<param name="includeProperties">error,rurl</param>
			</result>
		</action>
		
		<!-- 首页天气 -->
		<action name="index" class="indexInfoAction">
        	<result name="weathercityarea" type="json">
				<param name="includeProperties">wclist.*</param>
			</result>
			
			<result name="weatherscenety" type="json">
				<param name="includeProperties">wclist.*,wslist.*</param>
			</result>
			
			<result name="onlyCityLocation" type="json">
				<param name="includeProperties">cityId,cityName</param>
			</result>
			
			<result name="dwj" type="json">
				<param name="root">iff</param>
			</result>
		</action>
		
		<!-- 生活 -->
		<action name="live" class="liveAction">
			<result name="index" type="json">
				<param name="includeProperties">slist.*,total</param>
			</result>
			<result name="commentState" type="json">
				<param name="includeProperties">state</param>
			</result>
			
			<result name="sendCommentState" type="json">
				<param name="includeProperties">state</param>
			</result>
			
			<result name="sendError" type="json">
				<param name="includeProperties">resultCode</param>
			</result>
			
			<result name="sms" type="json">
				<param name="includeProperties">resultCode</param>
			</result>
			
			<result name="merchantCouponSend" type="json">
				<param name="includeProperties">resultCode</param>
			</result>
			
			<result name="commentSend" type="json">
				<param name="includeProperties">state</param>
			</result>
			<result name="upLoadPic" type="json">
				<param name="includeProperties">state</param>
			</result>
			<result name="jiucuo" type="json">
				<param name="includeProperties">state</param>
			</result>
		</action>
		
		
		<!-- 城市查询接口 -->
		<action name="city" class="cityAction">
        	<result name="success" type="json">
				<param name="root">result</param>
			</result>
        	
		</action>
		
		<!-- 软件频道接口 -->
		<action name="soft" class="softAction">
        	<result name="success" type="json">
				<param name="root">urlInfo</param>
			</result>
        	
		</action>
		<!-- 高考频道接口 -->
		<action name="gk" class="gaoKaoAction">
        	<result name="schoollist" type="json">
				<param name="root">ab</param>
			</result>
        	
        	<result name="res_historyscore" type="json">
				<param name="root">aab</param>
			</result>
        	<result name="res_majorscore" type="json">
				<param name="root">sab</param>
			</result>
        	
		</action>
		
		<!-- 优惠券-->
		<action name="coupon" class="couponAction">
			<result name="index" type="json">
				<param name="includeProperties">slist.*,total</param>
			</result>
		</action>
		<!-- 青奥会-->
		<action name="yog" class="yogAction">
			<result name="item_list" type="json">
				<param name="root">items</param>
			</result>
			<result name="schedulelist" type="json">
				<param name="root">match</param>
			</result>
		</action>
		
		
		<!-- 彩票
		<action name="cp" class="lotteryAction">
			<result name="success" type="json">
				<param name="root">result</param>
			</result>
		</action>
		-->
    </package>
</struts>
