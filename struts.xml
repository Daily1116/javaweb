			<result name="specialdetail" type="freemarker">/WEB-INF/www/live/topic.ftl</result>
			<result name="imageList" type="freemarker">/WEB-INF/www/live/all_pics.ftl</result>
			<result name="album" type="freemarker">/WEB-INF/www/live/album.ftl</result>
		</action>
		
		
		<!-- 网址导航 -->
		<action name="nav" class="navAction">
			<result name="index" type="freemarker">/WEB-INF/wap/nav/index.ftl</result>
			<result name="index_color" type="freemarker">/WEB-INF/wap/nav/index_color.ftl</result>
			<result name="index_color_b" type="freemarker">/WEB-INF/wap/nav/index_color_b.ftl</result>
		</action>
		
		<!-- 优惠券-->
		<action name="coupon" class="couponAction">
			<result name="index" type="freemarker">/WEB-INF/wap/andlive-coupon/index_list.ftl</result>
			<result name="changecity" type="freemarker">/WEB-INF/wap/andlive-coupon/change_city.ftl</result>
		</action>
		
		<!-- 快递-->
		<action name="kd" class="kdAction">
			<result name="index" type="freemarker">/WEB-INF/wap/kuaidi/fastmail.ftl</result>
			<result name="detail" type="freemarker">/WEB-INF/wap/kuaidi/fastmail_detail.ftl</result>
		</action>
		
		<!-- 彩票-->
		<action name="cp" class="lotteryAction">
			<result name="index" type="freemarker">/WEB-INF/wap/lottery/lottery.ftl</result>
			<result name="detail" type="freemarker">/WEB-INF/wap/lottery/lottery_detail.ftl</result>
		</action>
		
		<!-- 影讯-->
		<action name="mv" class="movieAction">
			<result name="index" type="freemarker">/WEB-INF/wap/lottery/lottery.ftl</result>
			<result name="detail" type="freemarker">/WEB-INF/wap/lottery/lottery_detail.ftl</result>
		</action>
		
		<!-- 软件频道 -->
		<action name="soft" class="softAction">
			<result name="soft_lastlist" type="freemarker">/WEB-INF/wap/soft/soft_lastlist.ftl</result>
