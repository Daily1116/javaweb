			<result name="soft_classlist" type="freemarker">/WEB-INF/wap/soft/soft_classlist.ftl</result>
			<result name="soft_list" type="freemarker">/WEB-INF/wap/soft/soft_list.ftl</result>
			<result name="soft_tophotlist" type="freemarker">/WEB-INF/wap/soft/soft_tophotlist.ftl</result>
			<result name="soft_topuplist" type="freemarker">/WEB-INF/wap/soft/soft_topuplist.ftl</result>
			<result name="soft_groomlist" type="freemarker">/WEB-INF/wap/soft/soft_groomlist.ftl</result>
		</action>
		
		<!-- 高考查分 -->
		<action name="gk" class="gaoKaoAction">
			<result name="search_by_school" type="freemarker">/WEB-INF/wap/gaokao/search_by_school.ftl</result>
			<result name="search_by_area" type="freemarker">/WEB-INF/wap/gaokao/search_by_area.ftl</result>
			<result name="choose_city" type="freemarker">/WEB-INF/wap/gaokao/choose_city.ftl</result>
			<result name="schoollist" type="freemarker">/WEB-INF/wap/gaokao/schoollist.ftl</result>
			<result name="res_scoreline" type="freemarker">/WEB-INF/wap/gaokao/res_scoreline.ftl</result>
			<result name="declare" type="freemarker">/WEB-INF/wap/gaokao/declare.ftl</result>
			<result name="res_schoolinfo" type="freemarker">/WEB-INF/wap/gaokao/res_schoolinfo.ftl</result>
			<result name="res_historyscore" type="freemarker">/WEB-INF/wap/gaokao/res_historyscore.ftl</result>
